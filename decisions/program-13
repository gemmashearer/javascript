<body>
	<script type="text/javascript">
	var myAge = Number(prompt("Enter your age",30));

	if (myAge >= 0 && myAge <= 10)
	{
		document.write("myAge is between 0 and 10<br />");
	}

	if ( !(myAge >= 0 && myAge <= 10) ) 
	{
		document.write("myAge is NOT between 0 and 10<br />");
	}
	
	if (myAge >=80 || myAge <= 10)
	{
		document.write("myAge is 80 or above OR 10 or below");
	}

	if ( (myAge >= 30 && myAge <= 39) || (myAge >= 80 && myAge <= 89))
	{
		document.write("myAge 30 or above but 39 or less or myAge is 80 or above but 89 or less");
	}
	</script>
</body>
</html>